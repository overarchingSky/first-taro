{
  "name": "first-taro",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "redux",
    "typescript": true,
    "css": "less"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "test": "jest",
    "weapp": "taro build --type weapp",
    "swan": "taro build --type swan",
    "alipay": "taro build --type alipay",
    "tt": "taro build --type tt",
    "h5": "taro build --type h5",
    "rn": "taro build --type rn",
    "build:weapp": "npm run weapp --mode=prod",
    "build:swan": "npm run swan --mode=prod",
    "build:alipay": "npm run alipay --mode=prod",
    "build:tt": "npm run tt --mode=prod",
    "build:h5": "npm run h5 --mode=prod",
    "build:rn": "npm run rn --mode=prod",
    "dev:weapp": "npm run weapp --mode=dev -- --watch",
    "dev:swan": "npm run swan --mode=dev -- --watch",
    "dev:alipay": "npm run alipay --mode=dev -- --watch",
    "dev:tt": "npm run tt --mode=dev -- --watch",
    "dev:h5": "npm run h5 --mode=prod -- --watch ",
    "dev:rn": "npm run rn --mode=dev -- --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/async-await": "1.3.16",
    "@tarojs/components": "1.3.16",
    "@tarojs/redux": "1.3.16",
    "@tarojs/redux-h5": "1.3.16",
    "@tarojs/router": "1.3.16",
    "@tarojs/taro": "1.3.16",
    "@tarojs/taro-alipay": "1.3.16",
    "@tarojs/taro-h5": "1.3.16",
    "@tarojs/taro-swan": "1.3.16",
    "@tarojs/taro-tt": "1.3.16",
    "@tarojs/taro-weapp": "1.3.16",
    "babel-jest": "^24.9.0",
    "conventional-changelog-cli": "^2.0.23",
    "dotenv": "^8.1.0",
    "dotenv-expand": "^5.1.0",
    "minimist": "^1.2.0",
    "nerv-devtools": "^1.4.0",
    "nervjs": "^1.4.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3",
    "standard-changelog": "^2.0.13",
    "taro-ui": "^2.2.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.6.0",
    "@tarojs/plugin-babel": "1.3.16",
    "@tarojs/plugin-csso": "1.3.16",
    "@tarojs/plugin-less": "1.3.16",
    "@tarojs/plugin-uglifyjs": "1.3.16",
    "@tarojs/webpack-runner": "1.3.16",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.4.8",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-config-taro": "1.3.16",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-taro": "1.3.16",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "1.3.16",
    "stylelint-taro-rn": "1.3.16",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg":"exec < /dev/tty &&  npx git cz --hook && vi:q || true;"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  }
}
